kbd 
  background-color #ccc
  border-radius 3px
  outline 1px solid #333
  box-shadow 0 1px 1px rgba(0, 0, 0, .2), 0 2px 0 0 rgba(255, 255, 255, .7) inset
  color #333
  display inline-block
  font-size .75rem
  font-weight 100
  // line-height 1
  padding 0 .5rem
  white-space nowrap

